             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             Height="32" Width="32">
    <Grid Width="42" Height="42" Margin="-2,-2,0,0">
        <Image x:Name="ChampionImage" Margin="0,0,10,10">
            <!--PUSH-->
            <Image.Clip>
                <RectangleGeometry Rect="2,2,38,38" />
            </Image.Clip>
        </Image>
    </Grid>
</UserControl>